generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Cards {
  Id            String @id @default(cuid())
  Question      String
  Answer        String
  Understanding Float      @default(0.5)
}